size(500, 500); // width, height
background(255, 0, 0); // RGB or greyscale
stroke(255, 0, 255); 
fill(255, 0, 100);
line(10, 10, 50, 50); // x1, y1, x2, y2
ellipse(100, 100, 90, 90); // cx, cy, width, height
rect(30, 100, 10, 10); // tl, tr, width, height
stroke(0, 0, 255);
point(50, 50); // x, y
triangle(100, 200, 200, 50, 30, 250); // vertices of the corners of the triangle
arc(50, 50, 100, 100, 0, HALF_PI); // cx, cy, width, height, start angle and end angle.